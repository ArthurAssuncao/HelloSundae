{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["react"],
  "extends": [".eslintrc-base.json"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true,
    "es2020": true,
    "commonjs": true
  }
}
